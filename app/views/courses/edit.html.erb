<header class="aui-page-header">
  <div class="aui-page-header-inner">
    <div class="aui-page-header-main">
      <h1>Edit Course</h1>
    </div>
    <div class="aui-page-header-actions">

    </div>
  </div>
</header>
<div class="aui-page-panel">
  <div class="aui-page-panel-inner">
    <div class="aui-page-panel-content">
      <%= form_for :course, url: course_path(@course), method: :post, :html => { :class => 'aui' } do |f| %>

          <%= error_messages_for 'course' %>
          <%= render :partial => 'courses/form', :locals => { :f => f } %>

          <div class="buttons-container">
            <div class="buttons">
              <%= f.submit 'Save Changes', :class => 'button submit' %>
              <%= link_to 'Cancel', courses_path, :class => 'cancel' %>
            </div>
          </div>

      <% end %>
    </div>
  </div>
</div>