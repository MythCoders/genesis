<div class="generalAdminHeading aui-page-header">
  <div class="aui-page-header-inner">
    <div class="aui-page-header-main">
      <ol class="aui-nav aui-nav-horizontal">
        <li>
          <h1>
            New School Year for <%= @model.school.name %>
          </h1>
        </li>
      </ol>
    </div>
  </div>
</div>
<div class="aui-page-panel">
  <div class="aui-page-panel-inner">
    <div class="aui-page-panel-content">
      <%= form_for :model, url: school, :html => { :class => 'aui' } do |f| %>

          <%= render :partial => 'schools/form', :locals => {:f => f} %>

          <div class="buttons-container">
            <div class="buttons">
              <%= f.submit 'Create', :class => 'button submit' %>
              <%= link_to 'Cancel', districts_path, :class => 'cancel' %>
            </div>
          </div>

      <% end %>
    </div>
  </div>
</div>